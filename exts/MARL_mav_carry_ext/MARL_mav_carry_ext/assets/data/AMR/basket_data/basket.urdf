<?xml version="1.0" ?>
<robot name="basket">
<link name="load_link">
    <visual name="visual">
      <geometry>
        <mesh filename="basket.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <material>
      <color rgba="0.5 0.5 0.5 0.5"/>
    </material>
    <collision name="collision">
      <geometry>
        <mesh filename="basket.dae" scale="1 1 1"/>
        <!-- <box size="0.55 0.45 0.15"/> -->
      </geometry>
    </collision>
    <inertial>
      <mass value="1.4"/>
      <origin rpy="0 0 0" xyz="0 0 0.06"/>
      <inertia ixx="0.02625" ixy="0" ixz="0" iyy="0.03791666666666667" iyz="0" izz="0.05891666666666667"/>
    </inertial>
  </link>
  <!-- odometry link -->
  <link name="load/odometry_sensor_link">
    <inertial>
      <inertia ixx="0.00001" ixy="0.0" ixz="0.0" iyy="0.00001" iyz="0.0" izz="0.00001"/>
      <mass value="1e-05"/>
      <!-- [kg] -->
    </inertial>
  </link>
  <!-- odometry joint -->
  <joint name="load/odometry_sensor_joint" type="revolute">
    <parent link="load_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.08"/>
    <child link="load/odometry_sensor_link"/>
    <limit effort="0" lower="0" upper="0" velocity="0"/>
  </joint>
</robot>
