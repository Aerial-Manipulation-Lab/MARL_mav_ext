<?xml version="1.0" ?>
<robot name="Falcon">
  <link name="Falcon/base_link"/>
  <joint name="Falcon/base_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.03"/>
    <parent link="Falcon/base_link"/>
    <child link="Falcon/base_link_inertia"/>
  </joint>
  <link name="Falcon/base_link_inertia">
